{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../src",
    "distDir": "../src",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "zqc_launcher",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "dialog": {
        "open": true
      },
      "path": {
        "all": true
      },
      "fs": {
        "all": true,
        "scope": [
          "**",
          "$APP/*",
          "$APPDATA/*"
        ]
      },
      "shell": {
        "all": true,
        "scope": [
          {
            "name": "run-docker-compose",
            "cmd": "docker-compose",
            "args": true
          },
          {
            "name": "run-docker-compose-down",
            "cmd": "docker-compose",
            "args": [
              "down"
            ]
          },
          {
            "name": "run-docker-compose-pull",
            "cmd": "docker-compose",
            "args": [
              "pull"
            ]
          },
          {
            "name": "run-docker-compose-up",
            "cmd": "docker-compose",
            "args": [
              "up",
              "-d"
            ]
          }
        ]
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.zqc.prod",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "zqc_launcher",
        "width": 800,
        "height": 600
      }
    ]
  }
}